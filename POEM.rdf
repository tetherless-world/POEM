    <!-- http://purl.org/twc/poem#canBeExperiencedAs -->

    <owl:ObjectProperty rdf:about="http://purl.org/twc/poem#canBeExperiencedAs">
        <owl:inverseOf rdf:resource="http://purl.org/twc/poem#isExperienceOf"/>
        <rdfs:range rdf:resource="http://purl.org/twc/poem#SymptomExperience"/>
        <rdfs:label>can be experienced as</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/twc/poem#canRespondTo -->

    <owl:ObjectProperty rdf:about="http://purl.org/twc/poem#canRespondTo">
        <owl:inverseOf rdf:resource="http://purl.org/twc/poem#hasPossibleRespondent"/>
        <rdfs:domain rdf:resource="http://semanticscience.org/resource/SIO_000485"/>
        <rdfs:range rdf:resource="http://hadatac.org/ont/vstoi#Questionnaire"/>
        <rdfs:label>can respond to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/twc/poem#detects -->

    <owl:ObjectProperty rdf:about="http://purl.org/twc/poem#detects">
        <owl:inverseOf rdf:resource="http://purl.org/twc/poem#isDetectedBy"/>
        <rdfs:domain rdf:resource="http://hadatac.org/ont/vstoi#Detector"/>
        <rdfs:label>detects</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/twc/poem#estimatesCondition -->

    <owl:ObjectProperty rdf:about="http://purl.org/twc/poem#estimatesCondition">
        <rdfs:domain rdf:resource="http://purl.org/twc/poem#SyndromeScale"/>
        <rdfs:range rdf:resource="http://purl.org/twc/poem#Condition"/>
        <rdfs:label>estimates condition</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/twc/poem#hasPossibleRespondent -->

    <owl:ObjectProperty rdf:about="http://purl.org/twc/poem#hasPossibleRespondent">
        <rdfs:domain rdf:resource="http://hadatac.org/ont/vstoi#Questionnaire"/>
        <rdfs:range rdf:resource="http://semanticscience.org/resource/SIO_000485"/>
        <rdfs:label>has possible respondent</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/twc/poem#hasScale -->

    <owl:ObjectProperty rdf:about="http://purl.org/twc/poem#hasScale">
        <rdfs:subPropertyOf rdf:resource="http://semanticscience.org/resource/SIO_000028"/>
        <owl:inverseOf rdf:resource="http://purl.org/twc/poem#isScaleIn"/>
        <rdfs:domain rdf:resource="http://purl.org/twc/poem#PsychometricQuestionnaire"/>
        <rdfs:range rdf:resource="http://purl.org/twc/poem#QuestionnaireScale"/>
        <rdfs:label>has scale</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/twc/poem#hasSubscale -->

    <owl:ObjectProperty rdf:about="http://purl.org/twc/poem#hasSubscale">
        <rdfs:subPropertyOf rdf:resource="http://semanticscience.org/resource/SIO_000028"/>
        <owl:inverseOf rdf:resource="http://purl.org/twc/poem#isSubscaleOf"/>
        <rdfs:domain rdf:resource="http://purl.org/twc/poem#CompositeScale"/>
        <rdfs:range rdf:resource="http://purl.org/twc/poem#SyndromeScale"/>
        <rdfs:label>has subscale</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/twc/poem#hasSymptom -->

    <owl:ObjectProperty rdf:about="http://purl.org/twc/poem#hasSymptom">
        <owl:inverseOf rdf:resource="http://purl.org/twc/poem#isSymptomOf"/>
        <rdfs:range rdf:resource="http://purl.org/twc/poem#Symptom"/>
        <rdfs:label>has symptom</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/twc/poem#hasTarget -->

    <owl:ObjectProperty rdf:about="http://purl.org/twc/poem#hasTarget">
