    </owl:ObjectProperty>
    


    <!-- http://purl.org/twc/poem#isScaleIn -->

    <owl:ObjectProperty rdf:about="http://purl.org/twc/poem#isScaleIn">
        <rdfs:subPropertyOf rdf:resource="http://semanticscience.org/resource/SIO_000068"/>
        <rdfs:label>is scale in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/twc/poem#isSubjectOfScale -->

    <owl:ObjectProperty rdf:about="http://purl.org/twc/poem#isSubjectOfScale">
        <rdfs:subPropertyOf rdf:resource="http://semanticscience.org/resource/SIO_000629"/>
        <rdfs:domain rdf:resource="http://purl.org/twc/poem#Condition"/>
        <rdfs:range rdf:resource="http://purl.org/twc/poem#SyndromeScale"/>
        <rdfs:label>is subject of scale</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/twc/poem#isSubscaleOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/twc/poem#isSubscaleOf">
        <rdfs:subPropertyOf rdf:resource="http://semanticscience.org/resource/SIO_000068"/>
