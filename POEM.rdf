    </owl:ObjectProperty>
    


    <!-- http://purl.org/twc/poem#isSymptomOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/twc/poem#isSymptomOf">
        <rdfs:domain rdf:resource="http://purl.org/twc/poem#Symptom"/>
