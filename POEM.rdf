        <rdfs:range rdf:resource="http://semanticscience.org/resource/SIO_000485"/>
        <rdfs:label>has target</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/twc/poem#indicatesExperience -->

    <owl:ObjectProperty rdf:about="http://purl.org/twc/poem#indicatesExperience">
        <owl:inverseOf rdf:resource="http://purl.org/twc/poem#isIndicatedBy"/>
        <rdfs:domain rdf:resource="http://purl.org/twc/poem#SymptomExperience"/>
        <rdfs:label>indicates experience</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/twc/poem#isAboutCondition -->

    <owl:ObjectProperty rdf:about="http://purl.org/twc/poem#isAboutCondition">
        <rdfs:subPropertyOf rdf:resource="http://semanticscience.org/resource/SIO_000332"/>
        <owl:inverseOf rdf:resource="http://purl.org/twc/poem#isSubjectOfScale"/>
        <rdfs:domain rdf:resource="http://purl.org/twc/poem#SyndromeScale"/>
        <rdfs:range rdf:resource="http://purl.org/twc/poem#Condition"/>
        <rdfs:label>is about condition</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/twc/poem#isDetectedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/twc/poem#isDetectedBy">
