    </owl:ObjectProperty>
    


    <!-- http://purl.org/twc/poem#isIndicatedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/twc/poem#isIndicatedBy">
        <rdfs:range rdf:resource="http://purl.org/twc/poem#SymptomExperience"/>
