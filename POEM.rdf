    </owl:ObjectProperty>
    


    <!-- http://purl.org/twc/poem#isExperienceOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/twc/poem#isExperienceOf">
        <rdfs:domain rdf:resource="http://purl.org/twc/poem#SymptomExperience"/>
