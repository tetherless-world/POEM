@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix csvw: <http://www.w3.org/ns/csvw#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix : <http://example.org/rules/> .
@prefix schema: <http://schema.org/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix poem: <http://purl.org/twc/poem/> .
@prefix vstoi: <http://purl.org/twc/vstoi/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

# Mapping for MethodTypes (data/xml/93_MethodTypes.xml)
:TriplesMap a rr:TriplesMap;
  rml:logicalSource [
    rml:source "data/xml/93_MethodTypes.xml";
    rml:referenceFormulation ql:XPath;
    rml:iterator "/dataroot/_x0039_3_MethodTypes"
  ];
  rr:subjectMap [
    rr:template "http://purl.org/twc/poem/individual/methodType/{MethodTypeID}"
  ];
  rr:predicateObjectMap [
    rr:predicate rdfs:subClassOf;
    rr:objectMap [ rr:constant prov:Activity ];
  ];
  rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [ rml:reference "MethodName" ];
  ];
  rr:predicateObjectMap [
    rr:predicate skos:definition;
    rr:objectMap [ rml:reference "MethodDefinition" ];
  ].
