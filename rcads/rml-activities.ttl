@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix csvw: <http://www.w3.org/ns/csvw#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix : <http://example.org/rules/> .
@prefix schema: <http://schema.org/> .
@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix poem: <http://purl.org/twc/poem/> .
@prefix vstoi: <http://purl.org/twc/vstoi/> .
@prefix fhir: <http://hl7.org/fhir/> .
@prefix sio: <http://semanticscience.org/resource/> .
@prefix prov: <http://www.w3.org/ns/prov#> .

:TriplesMap a rr:TriplesMap;
  rml:logicalSource [
    rml:source "data/xml/03_Activities.xml";
    rml:referenceFormulation ql:XPath;
    rml:iterator "/dataroot/_x0030_3_Activities"
  ].

:TriplesMap2 a rr:TriplesMap;
  rml:logicalSource [
    rml:source "data/xml/05_EntityGroupMap.xml";
    rml:referenceFormulation ql:XPath;
    rml:iterator "/dataroot/_x0030_5_EntityGroupMap"
  ].

:TriplesMapAgents a rr:TriplesMap;
  rml:logicalSource [
    rml:source "data/xml/81_Agents.xml";
    rml:referenceFormulation ql:XPath;
    rml:iterator "/dataroot/_x0038_1_Agents"
  ].

:TriplesMapAgentPersonMap a rr:TriplesMap;
  rml:logicalSource [
    rml:source "data/xml/82_AgentPersonMap.xml";
    rml:referenceFormulation ql:XPath;
    rml:iterator "/dataroot/_x0038_2_AgentPersonMap"
  ].

:TriplesMapPersons a rr:TriplesMap;
  rml:logicalSource [
    rml:source "data/xml/80_Persons.xml";
    rml:referenceFormulation ql:XPath;
    rml:iterator "/dataroot/_x0038_0_Persons"
  ].

:TriplesMap rr:subjectMap [
  rr:template "http://purl.org/twc/poem/individual/activity/{ActivityID}"
].

:TriplesMap2 rr:subjectMap [
  rr:template "http://purl.org/twc/poem/individual/instrument/{EntityID}"
].

:TriplesMapAgents rr:subjectMap [
  rr:template "http://purl.org/twc/poem/individual/agent/{AgentID}"
].

:TriplesMapAgentPersonMap rr:subjectMap [
  rr:template "http://purl.org/twc/poem/individual/agentperson/{AgentPersonMapID}"
].

:TriplesMapPersons rr:subjectMap [
  rr:template "http://purl.org/twc/poem/individual/person/{PersonID}"
].

:TriplesMap rr:predicateObjectMap [
  rr:predicate rdf:type;
  rr:objectMap [ rr:constant prov:Activity ];
].

:TriplesMap rr:predicateObjectMap [
  rr:predicate rdfs:label;
  rr:objectMap [
    rml:reference "ActivityName"
  ]
].

# :TriplesMap rr:predicateObjectMap [
#   rr:predicate prov:used;
#   rr:objectMap [
#     rr:template "http://purl.org/twc/poem/individual/instrument/{UsedEntityID}"
#   ]
# ].

:TriplesMap rr:predicateObjectMap [
  rr:predicate prov:used;
  rr:objectMap [
    rr:parentTriplesMap :TriplesMap2;
    rr:joinCondition [
      rr:child "UsedEntityID";
      rr:parent "EntityGroupID";
    ];
  ];
].

:TriplesMap rr:predicateObjectMap [
  rr:predicate prov:generated;
  rr:objectMap [
    rr:parentTriplesMap :TriplesMap2;
    rr:joinCondition [
      rr:child "GenEntityID";
      rr:parent "EntityGroupID";
    ];
  ];
].

